    $(window).load(function() {
		$('#SpaceHolder').css('display','none');
        $('#slider').nivoSlider({
			effect:'slideInRight'
		});
		$('#SliderControls a').each(function(e){
			var index = $(this).parent().index();
			var text = $(this).html();
			var nref= index +1;
			//$('.nivo-control[rel="' +index+'"]').html(text);
			var thestring = '<p>' + text + '</p><span>' + nref+ '</span>';
			$('.nivo-control[rel="' +index+'"]').html(thestring);
		});
    });
	$(document).ready(function(){
		
		$('#SliderControls a').click(function(e){
			e.preventDefault();
			var index = $(this).parent().index();
			$('.nivo-control[rel="' +index+'"]').trigger("click");
			
			//alert($(this).text());
			
		});
		
	/*	$('#SliderControls a').each(function(e){
			var index = $(this).parent().index();
			var text = $(this).html();
			alert($('.nivo-control[rel="' +index+'"]').html());
			
		});*/
	});
	
</script>






